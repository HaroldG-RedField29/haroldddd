<style>
    body {
      background: rgb(0, 255, 242);
      font-family: Helvetica Neue, Helvetica, Arial, sans-serif;
      font-size: 14px;
      color: rgb(252, 27, 27);
      margin: 0;
      padding: 0;
    }

    .swiper-container {
      width: 100%;
      padding-top: 50px;
      padding-bottom: 50px;
    }

    .swiper-slide {
      background-position: center;
      background-size: cover;
      width: 300px;
      height: 500px;

    }
</style>

<div class="swiper-container">
    <div class="swiper-wrapper">
        {{# each photos}}
             <img src="{{imageURL}}" alt="" class="swiper-slide">
        {{/each}}
    </div>
    <!-- Add Pagination -->
    <div class="swiper-pagination"></div>
</div>